{
  "openapi": "3.0.0",
  "info": {
    "title": "KIKIWORKFLOW SERVER by node sequelizer",
    "description": "",
    "version": "1.0.0"
  },
  "servers": [
    {
      "url": "http://localhost:8011"
    },
    {
      "url": "http://kikibus.iptime.org:18011"
    },
    {
      "url": "https://workflow.kiki-bus.com"
    }
  ],
  "paths": {
    "/api/task/": {
      "get": {
        "summary": "task list.",
        "description": null,
        "tags": ["task"],
        "responses": {
          "200": {
            "description": "A JSON array of task info",
            "content": {
              "application/json": {
                "schema": {
                  "properties": {
                    "name": {
                      "type": "string",
                      "example": "휴가신청"
                    },
                    "desc": {
                      "type": "string",
                      "example": "휴가를 신청함"
                    },
                    "creator_id": {
                      "type": "int",
                      "example": 1
                    },
                    "create_date": {
                      "type": "datetime",
                      "example": "2022-01-01T00:00:00.000Z"
                    },

                    "status": {
                      "type": "string",
                      "example": "진행중"
                    },
                    "due_date": {
                      "type": "datetime",
                      "example": "2022-01-01T00:00:00.000Z"
                    }
                  }
                }
              }
            }
          },
          "400": {
            "description": "Bad Request"
          },
          "401": {
            "description": "Unauthorized"
          },
          "403": {
            "description": "Forbidden"
          }
        }
      }
    }
  },
  "components": {
    "securitySchemes": {
      "bearerAuth": {
        "type": "http",
        "scheme": "bearer",
        "in": "header",
        "bearerFormat": "JWT"
      }
    }
  }
}
